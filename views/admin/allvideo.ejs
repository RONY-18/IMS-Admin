<%- include("include/header.ejs") %>
      <div class="container-fluid">
        <div class="row">
            <%- include("include/sidebar.ejs") %> 
            <div class="col-lg-9 p-3">
                <div class="form-group">
                  
                  <input type="text"
                    class="form-control" name="" id="myInput" aria-describedby="helpId" placeholder="Search By Anything">
                  
                </div>
                <table class="table">
                    <thead>
                        <tr>
                        
                            <th>Course</th>
                            <th>Title</th>
                            <th>Link</th>
                            <th>Details</th>
                            <th>Action</th>
                        </tr>
                    </thead>
                    <tbody id="myTable">
                        <% for(var x=0;x< result3.length; x++){ %>
                        <tr>
                            
                            <% for(var y=0;y< result2.length; y++){ %>
                                <%if(result3[x].cid==result2[y].id){%>
                                <td><%= result2[y].name %></td>
                                <% } %>
                            <% } %>
                            <td><%= result3[x].title %></td>
                            <td>
                                <iframe width="200" height="100" src="<%= result3[x].link %>" title="Python 31st class(b)" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" allowfullscreen></iframe>
                            </td>
                            <td><%= result3[x].details %></td>
                            <td>
                               <a name="" id="" class="btn btn-success" href="/admin/editvideo/<%= result3[x].id %>" role="button">Edit</a>
                            </td>
                        </tr>
                        <% } %>
                    </tbody>
                </table>
            </div>
        </div>
      </div>
<%- include("include/footer.ejs") %>    