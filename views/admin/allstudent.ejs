<%- include("include/header.ejs") %>
      <div class="container-fluid">
        <div class="row">
            <%- include("include/sidebar.ejs") %> 
            <div class="col-lg-9 p-3">
                <div class="form-group">
                  
                  <input type="text"
                    class="form-control" name="" id="myInput" aria-describedby="helpId" placeholder="Search By Anything">
                  
                </div>
                <table class="table">
                    <thead>
                        <tr>
                            <th>Image</th>
                            <th>Name</th>
                            <th>Email</th>
                            <th>Phone</th>
                            <th>Gender</th>
                            <th>RegDate</th>
                            <th>Action</th>
                        </tr>
                    </thead>
                    <tbody id="myTable">
                        <% for(var x=0;x< result.length; x++){ %>
                        <tr>
                            <td scope="row"><img style="height: 50px;" src="/student/<%= result[x].image %>" class="img-fluid ${3|rounded-top,rounded-right,rounded-bottom,rounded-left,rounded-circle,|}" alt=""></td>
                            <td><%= result[x].name %></td>
                            <td><%= result[x].email %></td>
                            <td><%= result[x].phone %></td>
                            <td><%= result[x].gender %></td>
                            <td><%= result[x].created_at %></td>
                            <td>
                                <a name="" id="" class="btn btn-warning" href="/admin/viewstudent/<%= result[x].id %>" role="button">View</a>
                                <a name="" id="" class="btn btn-success" href="/admin/editstudent/<%= result[x].id %>" role="button">Edit</a>
                                <a name="" id="" class="btn btn-danger" href="/admin/deletestudent/<%= result[x].id %>" role="button">Delete</a>
                            </td>
                        </tr>
                        <% } %>
                    </tbody>
                </table>
            </div>
        </div>
      </div>
<%- include("include/footer.ejs") %>    